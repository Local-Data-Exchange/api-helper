{
    "name": "lde/api-helper",
    "description": "Laravel package to help consume REST and XML APIs with little effort.",
    "license": "MIT",
    "keywords": [
        "Laravel Third Party Api Call",
        "Api Helper",
        "third party call",
        "consume api",
        "laravel api"
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "php": ">=7.2.5",
        "spatie/array-to-xml": "^2.14.0",
        "adbario/php-dot-notation": "^2.x.dev"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Lde\\ApiHelper\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lde\\ApiHelper\\ApiHelperServiceProvider"
            ],
            "aliases": {
                "ApiHelper": "Lde\\ApiHelper\\ApiHelperFacade"
            }
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lde\\ApiHelper\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^8.5",
        "orchestra/testbench": "~3.6.0|~3.7.0|~3.8.0"
    }
}
